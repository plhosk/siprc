{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "semi": ["error", "never"],
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "jsx-a11y/anchor-is-valid": [ "error", { "components": [""] } ],
    "max-len": ["error", 120, 2, {
      "ignoreUrls": true,
      "ignoreComments": false,
      "ignoreRegExpLiterals": true,
      "ignoreStrings": true,
      "ignoreTemplateLiterals": true
    }]  },
  "env": {
    "browser": true,
    "node": true
  }
}
